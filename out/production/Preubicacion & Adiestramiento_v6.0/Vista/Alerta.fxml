<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="pane" opacity="0.0" prefHeight="105.0" prefWidth="391.0" style="-fx-background-radius: 20px; -fx-border-color: lightgray; -fx-border-radius: 20px;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox fx:id="barra" prefHeight="62.0" prefWidth="389.0">
         <children>
            <HBox>
               <children>
                  <ImageView fx:id="imgarriba" fitHeight="28.0" fitWidth="26.0" pickOnBounds="true" smooth="false">
                     <HBox.margin>
                        <Insets bottom="5.0" left="10.0" top="5.0" />
                     </HBox.margin>
                  </ImageView>
                  <Label fx:id="titulo" textFill="WHITE" wrapText="true">
                     <font>
                        <Font name="System Bold" size="20.0" />
                     </font>
                     <effect>
                        <DropShadow blurType="GAUSSIAN" />
                     </effect>
                     <HBox.margin>
                        <Insets bottom="3.0" left="10.0" top="7.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </VBox>
      <HBox alignment="CENTER_RIGHT" prefWidth="250.0" spacing="10.0">
         <children>
            <Label fx:id="sms" prefWidth="500.0" wrapText="true">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin>
               <font>
                  <Font name="Arial" size="18.0" />
               </font>
            </Label>
            <ImageView fx:id="img" fitHeight="42.0" fitWidth="43.0" pickOnBounds="true" smooth="false">
               <HBox.margin>
                  <Insets right="10.0" top="15.0" />
               </HBox.margin>
            </ImageView>
         </children>
         <padding>
            <Insets bottom="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
